{
  "name": "markdown-preview-enhanced",
  "main": "./out/src/extension",
  "version": "0.15.0",
  "description": "One of the 'BEST' markdown preview extensions for Atom editor!",
  "repository": "https://github.com/shd101wyy/markdown-preview-enhanced",
  "license": "NCSA",
  "engines": {
    "atom": ">=1.16.0"
  },
  "activationCommands": {
    "atom-workspace": "markdown-preview-enhanced:toggle"
  },
  "activationHooks": [
    "language-gfm:grammar-used",
    "language-gfm-enhanced:grammar-used",
    "language-markdown:grammar-used",
    "language-pfm:grammar-used"
  ],
  "files": [
    "assets",
    "images",
    "keymaps",
    "menu",
    "out",
    "styles",
    "LICENSE.md",
    "README.md",
    "markdown.png"
  ],
  "keywords": [
    "markdown",
    "presentation",
    "image helper",
    "katex",
    "mathjax",
    "mermaid",
    "plantuml",
    "wavedrom",
    "pdf",
    "html",
    "exporter",
    "phantomjs",
    "toc",
    "footer",
    "graphviz",
    "dot language",
    "ePub",
    "ebook",
    "pandoc",
    "beamer",
    "latex"
  ],
  "scripts": {
    "build": "nothing happened"
  },
  "dependencies": {
    "@shd101wyy/mume": "^0.2.7"
  },
  "devDependencies": {
    "@types/atom": "^1.21.8",
    "@types/jquery": "^3.2.12",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.31",
    "chinese-conv": "^1.0.1",
    "typescript": "^2.5.3"
  }
}
